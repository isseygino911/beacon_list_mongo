 <table >
    <thead>
        <tr>
            <th>No.</th>
            <th>Todo item</th>
            <th>status</th>
            <th>Actions</th>
        </tr>
    </thead>
    <Tbody>
        <% todos.map(el => { %>
            <tr style=" <%= el.status !== 'Completed' ? 'background-color: white; height: 15px': 'background-color:#b8d0bb; height: 45px' %>"> 
                <td> <%= el.id %> </td>
                <td> <%= el.title %> </td>
                <td> <%= el.status %> </td>
                <td style="display: inline-flex;">
                    <form action="/todo/todos/<%= el.id %>?_method=DELETE" method="POST">
                        <button type="submit" id="delete" > Delete </button>
                    </form>
                    <form action="/todo/todos/<%= el.id %>?_method=PUT" method="POST">
                        <button type="submit" id="finished">Finished</button>
                    </form>
                </td>    
            </tr>
        <% }) %>     
    </Tbody>
</table>